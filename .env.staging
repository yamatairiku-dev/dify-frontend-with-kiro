# Staging environment variables
VITE_NODE_ENV=staging

# API Configuration
VITE_API_BASE_URL=https://api-staging.example.com
VITE_DIFY_API_URL=https://dify-staging.example.com

# OAuth Configuration (Staging)
VITE_AZURE_CLIENT_ID=staging-azure-client-id
VITE_AZURE_TENANT_ID=staging-azure-tenant-id
VITE_GITHUB_CLIENT_ID=staging-github-client-id
VITE_GOOGLE_CLIENT_ID=staging-google-client-id

# OAuth Redirect URIs
VITE_OAUTH_REDIRECT_URI=https://staging.example.com/callback

# Security Configuration
VITE_SESSION_TIMEOUT=30
VITE_MAX_LOGIN_ATTEMPTS=3

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_MODE=false
VITE_LOG_LEVEL=info