# Production environment variables
VITE_NODE_ENV=production

# API Configuration
VITE_API_BASE_URL=https://api.example.com
VITE_DIFY_API_URL=https://dify.example.com

# OAuth Configuration (Production)
VITE_AZURE_CLIENT_ID=prod-azure-client-id
VITE_AZURE_TENANT_ID=prod-azure-tenant-id
VITE_GITHUB_CLIENT_ID=prod-github-client-id
VITE_GOOGLE_CLIENT_ID=prod-google-client-id

# OAuth Redirect URIs
VITE_OAUTH_REDIRECT_URI=https://app.example.com/callback

# Security Configuration
VITE_SESSION_TIMEOUT=15
VITE_MAX_LOGIN_ATTEMPTS=3

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_MODE=false
VITE_LOG_LEVEL=error