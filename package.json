{
  "name": "kiro",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:development": "VITE_MODE=development npm run build",
    "build:staging": "VITE_MODE=staging npm run build",
    "build:production": "VITE_MODE=production npm run build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:all": "npm run test && npm run test:integration",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "deploy": "./scripts/deploy.sh",
    "deploy:development": "./scripts/deploy.sh development",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:production": "./scripts/deploy.sh production",
    "docker:build": "docker build -t dify-workflow-frontend .",
    "docker:run": "docker run -p 8080:8080 dify-workflow-frontend",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "health-check": "curl -f http://localhost:8080/health || exit 1",
    "validate-env": "node -e \"const { validateEnvironmentConfig } = require('./dist/config/deployment.js'); const result = validateEnvironmentConfig(); if (!result.isValid) { console.error('Missing env vars:', result.missingVars); process.exit(1); } console.log('Environment validation passed');\""
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@react-router/dev": "^7.9.1",
    "@react-router/node": "^7.9.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "prettier": "^3.6.2",
    "terser": "^5.44.0",
    "ts-jest": "^29.4.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
