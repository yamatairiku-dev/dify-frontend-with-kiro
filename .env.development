# Development environment variables
VITE_NODE_ENV=development

# API Configuration
VITE_API_BASE_URL=http://localhost:3001
VITE_DIFY_API_URL=http://localhost:8080

# OAuth Configuration (Development)
VITE_AZURE_CLIENT_ID=dev-azure-client-id
VITE_AZURE_TENANT_ID=dev-azure-tenant-id
VITE_GITHUB_CLIENT_ID=dev-github-client-id
VITE_GOOGLE_CLIENT_ID=dev-google-client-id

# OAuth Redirect URIs
VITE_OAUTH_REDIRECT_URI=http://localhost:5173/callback

# Security Configuration
VITE_SESSION_TIMEOUT=60
VITE_MAX_LOGIN_ATTEMPTS=5

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug